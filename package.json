{
  "name": "@luchsamapparat/nim-cli",
  "version": "1.0.0",
  "description": "Command-line client for Nim game library",
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint --project .",
    "compile": "tsc --project .",
    "build": "run-s clean lint compile",
    "watch": "run-s clean dev",
    "dev": "run-p \"compile -- --watch\"",
    "start": "node ./dist/index"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luchsamapparat/nim-cli.git"
  },
  "keywords": [
    "nim",
    "game",
    "cli"
  ],
  "author": "Marvin Luchs <marvin@luchs.org>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luchsamapparat/nim-cli/issues"
  },
  "homepage": "https://github.com/luchsamapparat/nim-cli#readme",
  "dependencies": {
    "@luchsamapparat/nim": "^4.0.0",
    "colors": "^1.1.2",
    "inquirer": "^4.0.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/colors": "^1.1.3",
    "@types/inquirer": "0.0.36",
    "@types/lodash": "^4.14.91",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.8.0",
    "tslint-config-luchsamapparat": "^1.2.0",
    "typescript": "^2.6.2"
  }
}
